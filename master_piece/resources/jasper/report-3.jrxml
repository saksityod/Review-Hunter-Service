	<subDataset name="ChartPie" uuid="6fc527cf-8c93-462c-abc7-60268e352626">
		<parameter name="param_doctor_id" class="java.lang.Integer"/>
		<parameter name="param_start_date" class="java.lang.String"/>
		<parameter name="param_end_date" class="java.lang.String"/>
		<queryString>
			<![CDATA[select mp.procedure_name, ct.case_type, count(*) as cc
from patient_case pc, case_appointment ca, medical_procedure mp, case_type ct
where pc.case_id = ca.case_id
and pc.procedure_id = mp.procedure_id
and pc.case_type_id = ct.case_type_id
and ca.doctor_id = $P{param_doctor_id}
and ca.appointment_date between $P{param_start_date} and $P{param_end_date}
group by mp.procedure_name, ct.case_type
order by mp.procedure_name, ct.case_type]]>
		</queryString>
		<field name="procedure_name" class="java.lang.String">
			<fieldDescription><![CDATA[]]></fieldDescription>
		</field>
		<field name="case_type" class="java.lang.String">
			<fieldDescription><![CDATA[]]></fieldDescription>
		</field>
		<field name="cc" class="java.lang.Long"/>
	</subDataset>
	<subDataset name="OR" uuid="5d507bc4-da32-4ecf-84be-c698f6785bae">
		<parameter name="param_start_date" class="java.lang.String"/>
		<parameter name="param_end_date" class="java.lang.String"/>
		<parameter name="param_doctor_id" class="java.lang.Integer"/>
		<queryString>
			<![CDATA[select pc.doctor_id, count(distinct pc.vn_no) as count2
from patient_case pc, case_stage cs
where pc.case_id = cs.case_id
and pc.doctor_id = $P{param_doctor_id}
and (from_stage_id  = 11 or (from_stage_id >= 14 and from_stage_id < 201))
and cs.actual_date between $P{param_start_date} and $P{param_end_date}]]>
		</queryString>
		<field name="doctor_id" class="java.lang.Integer">
			<fieldDescription><![CDATA[]]></fieldDescription>
		</field>
		<field name="count2" class="java.lang.Long"/>
	</subDataset>

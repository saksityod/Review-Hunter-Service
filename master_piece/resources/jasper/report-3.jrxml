	<queryString>
		<![CDATA[select att.appointment_type_name,
pt.patient_name,
CONCAT(DATE_FORMAT(ca.appointment_date,'%d'),'/',DATE_FORMAT(ca.appointment_date,'%m'),'/',DATE_FORMAT(ca.appointment_date,'%Y')+543,' ',TIME(ca.appointment_date)) appointment_date,
ca.is_vdo_product,
ca.is_picture_product,
ca.is_vdo_rh,
ca.is_picture_rh,
dtt.doctor_name,
count(*) as count1
from case_appointment ca
inner join appointment_type att on att.appointment_type_id = ca.appointment_type_id
inner join patient_case pc on pc.case_id = ca.case_id
inner join patient pt on pt.patient_id = pc.patient_id
inner join doctor dtt on dtt.doctor_id = ca.doctor_id
where ca.doctor_id = $P{param_doctor_id}
and ca.appointment_date between $P{param_start_date} and $P{param_end_date}
group by att.appointment_type_name,
				pt.patient_name,
				ca.appointment_date,
				ca.is_vdo_product,
				ca.is_picture_product,
				ca.is_vdo_rh,
				ca.is_picture_rh,
				dtt.doctor_name]]>
	</queryString>
